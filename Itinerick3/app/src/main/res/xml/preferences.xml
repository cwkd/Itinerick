<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:id="@+id/checkDarkTheme"
        android:defaultValue="false"
        android:key="@string/checkDarkThemeKey"
        android:summaryOff="Normal"
        android:summaryOn="Dark"
        android:title="Change Theme" />
    <CheckBoxPreference
        android:id="@+id/checkWhackyName"
        android:defaultValue="false"
        android:key="@string/checkWhackyNameKey"
        android:summaryOff="Normal"
        android:summaryOn="Whacky"
        android:title="Change Name" />
</PreferenceScreen>